{
    "name": "ynn-ms-qrcode",
    "version": "0.0.0",
    "description": "A micro service for generating QR code based on Ynn",
    "main": "src/index.js",
    "scripts": {
        "start": "nodemon src",
        "test": "jest --forceExit",
        "test-watch": "jest --watch",
        "test-cov": "jest --coverage --runInBand --forceExit --detectOpenHandles",
        "lint": "eslint lib --cache --ignore-path .eslintignore",
        "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ynnjs/ynn-ms-qrcode.git"
    },
    "keywords": [
        "qr",
        "qrcode",
        "qr-code",
        "ms",
        "service",
        "barcode",
        "ynn"
    ],
    "author": "LvChengbin <lvchengbin59@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ynnjs/ynn-ms-qrcode/issues"
    },
    "homepage": "https://github.com/ynnjs/ynn-ms-qrcode#readme",
    "dependencies": {
        "qrcode": "^1.3.0",
        "ynn": "0.0.14"
    },
    "devDependencies": {
        "@lvchengbin/sleep": "0.0.2",
        "eslint": "^5.9.0",
        "jest": "^23.6.0",
        "nodemon": "^1.18.5",
        "pre-commit": "^1.2.2",
        "supertest": "^3.3.0"
    },
    "engines": {
        "node": ">=8.0"
    },
    "jest": {
        "testMatch": [
            "**/test/**/*.spec.js"
        ],
        "coverageReporters": [
            "text-summary",
            "text",
            "lcov"
        ],
        "collectCoverageFrom": [
            "lib/**/*.js"
        ],
        "testEnvironment": "node"
    }
}
